algoritmo "conteate10"

var
  N1, valor, contador: Inteiro
inicio
    contador <- 0
    escreva ("Quer contar até quanto? ")
    leia(valor)
    escreva ("Quer pular de quanto em quanto? ")
    leia (N1)
    enquanto (contador <= valor) faca
        escreval(contador)
        contador <- contador + N1
    FimEnquanto
    escreval("Terminei de contar")
fimalgoritmo
